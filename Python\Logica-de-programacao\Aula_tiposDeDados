      "cell_type": "code",
      "source": [
        "aluno = input('Digite o nome do aluno: ')\n",
        "primeiraNota = float(input('Digite a primeira nota: '))\n",
        "segundaNota = float(input('Digite a segunda nota: '))\n",
        "terceiraNota = float(input('Digite a terceira nota: '))\n",
        "\n",
        "mediaDeProvas = (primeiraNota + segundaNota + terceiraNota) / 3\n",
        "\n",
        "print('\\nA sua média: ')\n",
        "print(mediaDeProvas)\n",
        "\n",
        "if(mediaDeProvas < 4):\n",
        "  print('\\n' + aluno + ' SE FUDEU')\n",
        "elif(mediaDeProvas < 7):\n",
        "  print('\\n' + aluno + ' RECUPERAÇÃO')\n",
        "else:\n",
        "  print('\\n' + aluno + ' PASSOU')"
